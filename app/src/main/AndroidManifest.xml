<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tipntale.calicutuniversity">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/cuappicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cuappicon"
        android:hardwareAccelerated="true"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity"/>
        <activity android:name=".notificationActivity" />
        <activity android:name=".browserActivity" />
        <activity android:name=".timetableActivity" />
        <activity android:name=".sdenotificationActivity" />
        <activity android:name=".contactActivity" />
        <activity android:name=".pdfbrowserActivity" />
        <activity android:name=".settingsActivity" />
        <activity android:name=".aboutActivity" />
        <activity android:name=".feedbackActivity" />
        <activity android:name=".splashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-3940256099942544~3347511713"/>

    </application>

</manifest>